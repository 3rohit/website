<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">

<channel>
  <title>Sharecuts</title>
  <link>https://sharecuts.app</link>
  <description>A public repository of shortcuts for the Shortcuts app.</description>
  #for(card in cards) {
    <item>
    <title>#(card.shortcut.title)</title>
    <link>#(card.downloadLink)</link>
    <description>#(card.shortcut.summary)</description>
  </item>
  }
</channel>

</rss>